{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@api/*": [
        "src/common/api/*"
      ],
      "@common/*": [
        "src/common/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@mixin/*": [
        "src/core/mixins/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "src"
  ]
}