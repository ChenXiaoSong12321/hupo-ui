<block>
  <block wx:if="{{nodes.node == 'element'}}">
    <block wx:if="{{ nodes.tag == 'video' || nodes.tag == 'embed' || nodes.tag == 'img' || nodes.tag == 'br' || nodes.tag == 'button' || nodes.tag == 'li' || nodes.tag == 'a' || nodes.tag == 'table' || nodes.tag == 'tr' }}">
      <parse-video wx:if="{{nodes.tag == 'video' || nodes.tag == 'embed'}}" nodes="{{nodes}}"></parse-video>
      <parse-img wx:if="{{nodes.tag == 'img'}}" nodes="{{nodes}}"></parse-img>
      <parse-br wx:if="{{nodes.tag == 'br'}}" nodes="{{nodes}}"></parse-br>
      <parse-button wx:if="{{nodes.tag == 'button'}}" nodes="{{nodes}}"></parse-button>
      <parse-li wx:if="{{nodes.tag == 'li'}}" nodes="{{nodes}}"></parse-li>
      <parse-a wx:if="{{nodes.tag == 'a'}}" nodes="{{nodes}}"></parse-a>
      <parse-table wx:if="{{nodes.tag == 'table'}}" nodes="{{nodes}}"></parse-table>
      <parse-tr wx:if="{{nodes.tag == 'tr'}}" nodes="{{nodes}}"></parse-tr>
    </block>
    <block wx:elif="{{nodes.tagType == 'block'}}">
      <parse-block nodes="{{nodes}}"></parse-block>
    </block>
    <block wx:else>
      <parse-inline nodes="{{nodes}}"></parse-inline>
    </block>
  </block>
  <block wx:elif="{{nodes.node == 'text'}}">
    <parse-text nodes="{{nodes}}"></parse-text>
  </block>
</block>
